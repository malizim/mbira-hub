{
  "name": "mbira-recording-session",
  "version": "3.0.0",
  "description": "Advanced mbira recording session with real-time note detection and music sheet generation",
  "main": "server/index.js",
  "type": "module",
  "scripts": {
    "start": "node server/index.js",
    "dev": "node --watch server/index.js",
    "install-deps": "npm install",
    "setup": "node scripts/setup.js",
    "test": "node test.js",
    "deploy": "./deploy.sh",
    "deploy:shell": "sudo ./install.sh",
    "deploy:docker": "./docker-deploy.sh deploy",
    "deploy:test": "./test-deployment.sh",
    "docker:build": "docker compose build",
    "docker:up": "docker compose up -d",
    "docker:down": "docker compose down",
    "docker:logs": "docker compose logs -f",
    "docker:status": "docker compose ps"
  },
  "dependencies": {
    "@prisma/client": "^5.7.1",
    "bcrypt": "^5.1.1",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "fft-js": "^0.0.12",
    "fluent-ffmpeg": "^2.1.2",
    "jsonwebtoken": "^9.0.2",
    "jspdf": "^2.5.1",
    "multer": "^1.4.5-lts.1",
    "prisma": "^5.7.1",
    "socket.io": "^4.7.5",
    "uuid": "^9.0.1",
    "vexflow": "^4.2.2",
    "ws": "^8.14.2"
  },
  "devDependencies": {
    "nodemon": "^3.0.2"
  },
  "keywords": [
    "mbira",
    "recording",
    "music",
    "real-time",
    "note-detection",
    "music-sheet"
  ],
  "author": "Rhythm Keepers",
  "license": "MIT"
}
